int main()
{

#include <iostream>
#include <string>
using namespace std;

  int *P;
  // cout <<"after first:" << endl;
  P = new int;
  // cout<<"after second:"<<endl;
  *P = 3;
  // cout <<"after third:" << endl;
  P = NULL;  // the cell is gone
  // cout <<"after fourth:"<<endl;
  cout << *P << endl;
